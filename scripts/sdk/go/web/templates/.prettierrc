{
	"useTabs": true,
	"tabWidth": 2,
	"singleAttributePerLine": true,
	"endOfLine": "lf",
	"printWidth": 80,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "none",
	"plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-go-template"],
	"overrides": [{ "files": ["*.html"], "options": { "parser": "go-template" } }]
}
